{
  "expo": {
    "name": "bc",
    "slug": "bc",
    "version": "1.0.0",
    "sdkVersion": "48.0.0", // Pastikan versi SDK cocok dengan yang terinstal
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png", // Pastikan path ini benar
    "splash": {
      "image": "./assets/splash.png", // Pastikan path ini benar
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png", // Pastikan path ini benar
        "backgroundColor": "#ffffff"
      }
    }
  }
}
